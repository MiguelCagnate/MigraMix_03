{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|gif|json))",
      "dest": "/client/$1"
    },
    {
      "src": "/(.*\\.(html|ico))",
      "dest": "/client/$1"
    },
    {
      "src": "/",
      "dest": "/client/index.html"
    }
  ]
}
